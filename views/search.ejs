<link rel="stylesheet" href="/css/style.css">
<div class="author">
    <h1 class="author_heading">
        search results
    </h1>
</div>


    <section class="articles">
        <ul class="articles-ul">
            <% data.forEach(post => { %>
             
           
            <li>
                <a href="/post/<%= post._id %>" >
                    <span><%= post.title %></span>
                    <span class="date"><%= post.createdAt.toDateString()%></span>

                </a>
            </li>
            <% }) %>
        </ul>
</section>